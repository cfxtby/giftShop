<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <display-name>giftShop</display-name>

  <servlet>
    <servlet-name>welcome</servlet-name>
    <servlet-class>tby.com.gift.welcome</servlet-class>
    <load-on-startup>2</load-on-startup>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>welcome</servlet-name>
    <url-pattern>/welcome</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>goodInfo</servlet-name>
    <servlet-class>tby.com.gift.infoOfGood</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>goodInfo</servlet-name>
    <url-pattern>/goodInfo</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>access_token</servlet-name>
    <servlet-class>tby.com.gift.getAccesstoken</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>access_token</servlet-name>
    <url-pattern>/access_token</url-pattern>
  </servlet-mapping>
  
    <servlet>
    <servlet-name>register</servlet-name>
    <servlet-class>tby.com.Oauth.regist</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>register</servlet-name>
    <url-pattern>/register</url-pattern>
  </servlet-mapping>
  
  
   <servlet>
    <servlet-name>query</servlet-name>
    <servlet-class>tby.com.gift.query</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>query</servlet-name>
    <url-pattern>/query</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>shoppingCar</servlet-name>
    <servlet-class>tby.com.gift.shoppingCar</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>shoppingCar</servlet-name>
    <url-pattern>/shoppingCar</url-pattern>
  </servlet-mapping>
    <servlet>
    <servlet-name>dealOrder</servlet-name>
    <servlet-class>tby.com.gift.dealOrder</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>dealOrder</servlet-name>
    <url-pattern>/dealOrder</url-pattern>
  </servlet-mapping>
  
  
</web-app>